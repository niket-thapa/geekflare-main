{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "main/meet-experts",
  "title": "Meet Experts",
  "category": "main",
  "icon": "groups",
  "description": "Display authors/experts in a carousel with grouping",
  "keywords": ["authors", "experts", "carousel", "team"],
  "textdomain": "main",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css",
  "render": "file:./render.php",
  "attributes": {
    "heading": {
      "type": "string",
      "default": "Meet our experts"
    },
    "subheading": {
      "type": "string",
      "default": "Discover in-depth insights, reviews, and guides crafted by our team of industry experts."
    },
    "authors": {
      "type": "array",
      "default": []
    }
  },
  "supports": {
    "html": false,
    "align": ["wide", "full"]
  }
}


{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "main/final-verdict",
  "title": "Final Verdict",
  "category": "main",
  "icon": "editor-ul",
  "description": "Final verdict with perfect for and not ideal for sections",
  "keywords": ["verdict", "final", "perfect", "ideal"],
  "textdomain": "main",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css",
  "render": "file:./render.php",
  "attributes": {
    "heading": {
      "type": "string",
      "default": "Final Verdict"
    },
    "perfectForHeading": {
      "type": "string",
      "default": "Perfect for:"
    },
    "notIdealForHeading": {
      "type": "string",
      "default": "Not ideal for:"
    },
    "perfectFor": {
      "type": "array",
      "default": [""]
    },
    "notIdealFor": {
      "type": "array",
      "default": [""]
    }
  },
  "supports": {
    "html": false
  }
}
